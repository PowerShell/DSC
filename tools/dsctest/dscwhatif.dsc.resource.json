{
    "$schema": "https://aka.ms/dsc/schemas/v3/bundled/resource/manifest.json",
    "type": "Test/WhatIf",
    "version": "0.1.0",
    "get": {
        "executable": "dsctest",
        "args": [
            "whatif"
        ]
    },
    "set": {
        "executable": "dsctest",
        "args": [
            "whatif"
        ],
        "return": "state"
    },
    "whatIf": {
        "executable": "dsctest",
        "args": [
            "whatif",
            "-w"
        ],
        "return": "state"
    },
    "schema": {
        "command": {
            "executable": "dsctest",
            "args": [
                "schema",
                "-s",
                "what-if"
            ]
        }
    }
}
